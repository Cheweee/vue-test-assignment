    "version": "0.2.0",
    "configurations": [
        {
            "name": "development",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/VueTest/bin/Debug/netcoreapp2.2/VueTest.dll",
